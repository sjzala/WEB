{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "data/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/css/(.*)",
            "dest":"public/css/$1"
        },
        
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
 }
 
 
 